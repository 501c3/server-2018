{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.1.3",
        "facebook/graph-sdk": "^5.6",
        "google/recaptcha": "^1.1",
        "hwi/oauth-bundle": "^0.6.0",
        "knplabs/knp-paginator-bundle": "^2.7",
        "knpuniversity/oauth2-client-bundle": "^1.16",
        "league/oauth2-facebook": "^2.0",
        "league/oauth2-github": "^2.0",
        "league/oauth2-google": "^2.0",
        "league/oauth2-linkedin": "^2.1",
        "php-http/guzzle6-adapter": "^1.1",
        "php-http/httplug-bundle": "^1.8",
        "sensio/framework-extra-bundle": "^5.2",
        "sensiolabs/security-checker": "^4.1",
        "stevenmaguire/oauth2-paypal": "^2.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "^4.0",
        "symfony/console": "^4.0",
        "symfony/debug-pack": "^1.0",
        "symfony/event-dispatcher": "^4.0",
        "symfony/flex": "^1.0",
        "symfony/form": "^4.0",
        "symfony/framework-bundle": "^4.0",
        "symfony/lts": "^4@dev",
        "symfony/maker-bundle": "^1.0",
        "symfony/orm-pack": "^1.0",
        "symfony/security-bundle": "^4.0",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/thanks": "^1.0",
        "symfony/twig-bundle": "^4.0",
        "symfony/validator": "^4.0",
        "symfony/webpack-encore-pack": "^1.0",
        "symfony/yaml": "^4.0",
        "twig/twig": "~2.0"
    },
    "require-dev": {
        "behat/mink-extension": "^2.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/symfony2-extension": "^2.1",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "fzaninotto/faker": "^1.7",
        "nelmio/alice": "^3.1",
        "symfony/dotenv": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/profiler-pack": "^1.0",
        "symfony/web-server-bundle": "^4.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/",
            "Symfony\\Component\\Dotenv\\": "vendor/symfony/dotenv/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "security-checker security:check": "script"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C0CGHQAMF96FXD7V2W9J275P",
            "allow-contrib": false
        }
    }
}
